<Project Sdk="Microsoft.NET.Sdk">
<Sdk Name="Microsoft.Build.CentralPackageVersions" Version="2.1.3" />
  <PropertyGroup>
    <TargetFrameworks>net6.0</TargetFrameworks>
    <IsPackable>false</IsPackable>
    <IsPublishable>false</IsPublishable>
    <Configurations>Debug;Release</Configurations>
    <Platforms>AnyCPU</Platforms>
    <RootNamespace>DotNet.Testcontainers.Tests</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" />
    <PackageReference Include="Microsoft.Extensions.Hosting" />
    <PackageReference Include="coverlet.msbuild" />
    <PackageReference Include="Serilog.Extensions.Logging" />
    <PackageReference Include="Serilog.Settings.Configuration" />
    <PackageReference Include="Serilog.Sinks.Console" />
    <PackageReference Include="Serilog.Sinks.File" />
    <PackageReference Include="Serilog" />
    <PackageReference Include="xunit.runner.visualstudio" />
    <PackageReference Include="xunit" />
    <PackageReference Include="Azure.Data.Tables" />
    <PackageReference Include="Azure.Storage.Blobs" />
    <PackageReference Include="Azure.Storage.Queues" />
    <PackageReference Include="Confluent.Kafka" />
    <PackageReference Include="CouchbaseNetClient" />
    <PackageReference Include="Elastic.Clients.Elasticsearch" />
    <PackageReference Include="Microsoft.Azure.Cosmos" />
    <PackageReference Include="MongoDB.Driver" />
    <PackageReference Include="MyCouch" />
    <PackageReference Include="Neo4j.Driver" />
    <PackageReference Include="Npgsql" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" />
    <PackageReference Include="Pomelo.EntityFrameworkCore.MySql" />
    <PackageReference Include="RabbitMQ.Client" />
    <PackageReference Include="StackExchange.Redis" />
    <PackageReference Include="System.Data.SqlClient" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="$(SolutionDir)src/Testcontainers/Testcontainers.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\AzuriteTestcontainer\Testcontainers.Databases.AzuriteTestContainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\CosmosDbTestcontainer\Testcontainers.Databases.CosmosDbTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\CouchbaseTestcontainer\Testcontainers.Databases.CouchbaseTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\CouchdbTestcontainer\Testcontainers.Databases.CouchdbTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\ElasticsearchTestcontainer\Testcontainers.Databases.ElasticsearchTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\MariaDbTestcontainer\Testcontainers.Databases.MariaDbTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\MongoDbTestcontainer\Testcontainers.Databases.MongoDbTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\MsSqlTestcontainer\Testcontainers.Databases.MsSqlTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\MySqlTestcontainer\Testcontainers.Databases.MySqlTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\Neo4jTestcontainer\Testcontainers.Databases.Neo4jTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\OracleTestcontainer\Testcontainers.Databases.OracleTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\PostgreSqlTestcontainer\Testcontainers.Databases.PostgreSqlTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.Databases\RedisTestcontainer\Testcontainers.Databases.RedisTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.MessageBrokers\KafkaTestcontainer\Testcontainers.MessageBrokers.KafkaTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.MessageBrokers\LocalStackTestcontainer\Testcontainers.MessageBrokers.LocalStackTestcontainer.csproj" />
    <ProjectReference Include="..\..\src\Testcontainers.MessageBrokers\RabbitMqTestcontainer\Testcontainers.MessageBrokers.RabbitMqTestcontainer.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <None Update="xunit.runner.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <None Include="Assets/**/*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
